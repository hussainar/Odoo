<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        
        <template id="login_layout" inherit_id="web.login_layout"
            name="My Theme Login">
            <xpath expr="//div[@class='oe_single_form_footer']"
                position="replace">
                <div class="oe_single_form_footer" t-if="not disable_footer">
                    Powered by 
                    <a href="http://www.hussainar.com" target="_blank">
                        <span>Addonlabs</span>
                    </a>
                </div>
            </xpath>
        </template>
         <template id="menu_secondary" inherit_id="web.menu_secondary">
            <xpath expr="//div[@class='oe_footer']"
                position="replace">
                 <div class="oe_footer">
                Powered by <a href="http://www.hussainar.com" target="_blank"><span>Addonlabs</span></a>
            </div>
            </xpath>
        </template>
        
    </data>
</openerp>